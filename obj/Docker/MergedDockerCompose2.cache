services:
  olufemi-lineten:
    build:
      context: C:\Users\Olufemi\source\repos\Olufemi-LineTen
      dockerfile: Olufemi-LineTen/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Olufemi-LineTen
      target: base
    container_name: Olufemi-LineTen
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: olufemilineten:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/Olufemi-LineTen.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 51080
      target: 80
    - published: 51443
      target: 443
    tty: true
    volumes:
    - C:\Users\Olufemi\source\repos\Olufemi-LineTen\Olufemi-LineTen:/app:rw
    - C:\Users\Olufemi\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Olufemi\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Olufemi\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Olufemi\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Olufemi\source\repos\Olufemi-LineTen:/src:rw
version: '3.4'